<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" metadata-complete="true" version="3.0">
  
  <servlet>
    <servlet-name>vcs</servlet-name>
    <servlet-class>com.junrenge.servlet.ValidateColorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>vcs</servlet-name>
    <url-pattern>/vc</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>LoginSer</servlet-name>
    <servlet-class>com.junrenge.servlet.LoginSer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginSer</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>StudentSer</servlet-name>
    <servlet-class>com.junrenge.servlet.StudentSer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentSer</servlet-name>
    <url-pattern>/student</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>PlanSer</servlet-name>
    <servlet-class>com.junrenge.servlet.PlanSer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PlanSer</servlet-name>
    <url-pattern>/plan</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>ClassSer</servlet-name>
    <servlet-class>com.junrenge.servlet.ClassSer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ClassSer</servlet-name>
    <url-pattern>/class</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>SelectedClassSer</servlet-name>
    <servlet-class>com.junrenge.servlet.SelectedClassSer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SelectedClassSer</servlet-name>
    <url-pattern>/sc</url-pattern>
  </servlet-mapping>
  
  <filter>
  	<filter-name>EncodeFilter</filter-name>
  	<filter-class>com.junrenge.filter.EncodeFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>EncodeFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  
</web-app>